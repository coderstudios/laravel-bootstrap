{
    "name": "coderstudios/laravel-bootstrap",
    "description": "Laravel Bootstrap by Coder Studios",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "authors": [
        {
            "name": "Coder Studios",
            "email": "thedevelopers@coderstudios.com"
        }
    ],
    "keywords": [  
        "bootstrap",
        "laravel",
        "coderstudios"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CoderStudios\\LaravelBootstrap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoderStudios\\LaravelBootstrap\\LaravelBootstrapServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}